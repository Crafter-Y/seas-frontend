{
  "name": "csa-frontend",
  "version": "1.0.11",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\" \"expo start --web\" \"node proxy.js\"",
    "storybook": "cross-env STORYBOOK_ENABLED='true' expo start --port 8082",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "build:web": "expo export --platform web",
    "build:android": "eas build --platform android",
    "submit:android": "eas submit --platform android",
    "submit": "eas build --platform all --auto-submit",
    "serve": "cross-env SERVER_ID=58119 SERVE_LOCAL_NETWORK=1 node serve.js",
    "serve:prod": "node serve.js",
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "lint": "eslint .",
    "storybook-generate": "sb-rn-get-stories",
    "analyze": "cross-env EXPO_UNSTABLE_ATLAS=true expo start"
  },
  "dependencies": {
    "@expo/match-media": "^0.4.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-picker/picker": "2.9.0",
    "@shopify/flash-list": "1.7.1",
    "email-validator": "^2.0.4",
    "expo": "~52.0.18",
    "expo-calendar": "~14.0.5",
    "expo-checkbox": "~4.0.0",
    "expo-constants": "~17.0.3",
    "expo-image": "~2.0.3",
    "expo-linking": "~7.0.3",
    "expo-localization": "~16.0.0",
    "expo-mail-composer": "~14.0.1",
    "expo-print": "~14.0.2",
    "expo-router": "~4.0.11",
    "expo-screen-orientation": "~8.0.1",
    "expo-sharing": "~13.0.0",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "i18next": "^23.16.4",
    "jwt-decode": "^4.0.0",
    "pullstate": "^1.25.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.1.0",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.10.1",
    "react-native-paper-dates": "^0.18.12",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.19.13",
    "react-native-web-hooks": "^3.0.2",
    "react-responsive": "^9.0.2",
    "tailwind-rn": "^4.2.0",
    "twrnc": "^3.6.0",
    "weeknumber": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.22.11",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/slider": "4.5.5",
    "@storybook/addon-ondevice-actions": "^7.6.18",
    "@storybook/addon-ondevice-controls": "^7.6.18",
    "@storybook/manager-api": "^8.0.9",
    "@storybook/react-native": "^7.6.18",
    "@storybook/theming": "^8.0.9",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "expo-atlas": "^0.3.0",
    "postcss": "^8.4.21",
    "serve-handler": "^6.1.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.3.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@expo/match-media",
          "react-native-keyboard-aware-scroll-view",
          "react-native-paper-dates",
          "react-native-modal",
          "react-native-web-hooks"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
