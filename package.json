{
  "name": "csa-frontend",
  "version": "1.0.13",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\" \"expo start --web\" \"node proxy.js\"",
    "storybook": "cross-env STORYBOOK_ENABLED='true' expo start --port 8082 -c",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "build:web": "expo export --platform web",
    "build:android": "eas build --platform android",
    "submit:android": "eas submit --platform android",
    "submit": "eas build --platform all --auto-submit",
    "serve": "cross-env SERVER_ID=10101 SERVE_LOCAL_NETWORK=1 node serve.js",
    "serve:prod": "node serve.js",
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "lint": "expo lint",
    "storybook-generate": "sb-rn-get-stories",
    "analyze": "cross-env EXPO_UNSTABLE_ATLAS=true expo start",
    "test:e2e": "node e2e.js"
  },
  "dependencies": {
    "@expo/match-media": "^0.4.0",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "2.11.1",
    "@shopify/flash-list": "1.7.6",
    "classnames": "^2.5.1",
    "email-validator": "^2.0.4",
    "expo": "53.0.19",
    "expo-calendar": "~14.1.4",
    "expo-checkbox": "~4.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-image": "~2.3.2",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-mail-composer": "~14.1.5",
    "expo-print": "~14.1.4",
    "expo-router": "~5.1.3",
    "expo-screen-orientation": "~8.1.7",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "i18next": "^23.16.4",
    "jwt-decode": "^4.0.0",
    "nativewind": "^4.1.23",
    "pullstate": "^1.25.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.1.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-paper": "^5.10.1",
    "react-native-paper-dates": "^0.18.12",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.2.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.20.0",
    "react-native-web-hooks": "^3.0.2",
    "react-responsive": "^9.0.2",
    "tailwind-rn": "^4.2.0",
    "twrnc": "^3.6.0",
    "weeknumber": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.22.11",
    "@limegrass/eslint-plugin-import-alias": "^1.5.0",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/slider": "4.5.6",
    "@storybook/addon-ondevice-actions": "^8.5.3",
    "@storybook/addon-ondevice-controls": "^8.5.3",
    "@storybook/manager-api": "^8.5.2",
    "@storybook/react-native": "^8.5.3",
    "@storybook/react-native-web-vite": "^8.5.2",
    "@storybook/theming": "^8.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/react": "~19.0.10",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-prettier": "^5.2.3",
    "expo-atlas": "^0.4.0",
    "postcss": "^8.4.21",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "serve-handler": "^6.1.5",
    "storybook": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^6.0.11"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-keyboard-aware-scroll-view",
          "react-native-web-hooks",
          "jwt-decode"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
